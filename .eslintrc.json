{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false,
        "varsIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [],
  "ignorePatterns": ["*.cjs", "*.min.js"],
  "parserOptions": {
    "ecmaVersion": 15,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  }
}
