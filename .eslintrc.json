{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "none",
        "ignoreRestSiblings": false,
        "varsIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [],
  "ignorePatterns": ["*.cjs", "*.min.js"],
  "parserOptions": {
    "ecmaVersion": 14,
    "sourceType": "module"
  }
}
